{
  "all": true,
  "check-coverage": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "dist/**",
    "certs/**",
    "test/**",
    "coverage/**",
    "src/db/migrations/**",
    "src/db/models/**",
    "src/**/*.test.ts",
    "env/**",
    "src/index.ts",
    ".test-coverage/**"
  ],
  "skip-full": true,
  "sourceMap": true,
  "instrument": true,
  "require": ["@babel/register"],
  "reporter": ["text", "html", "lcov"],
  "lines": 50,
  "branches": 50,
  "functions": 50,
  "statements": 50,
  "report-dir": "./.test-coverage",
  "temp-dir": "./.nyc_output"
}
